<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Sualiado</title>
    <script type="text/javascript" src="https://cdn.fusioncharts.com/fusioncharts/latest/fusioncharts.js"></script>
    <script type="text/javascript" src="https://cdn.fusioncharts.com/fusioncharts/latest/themes/fusioncharts.theme.fusion.js"></script>
    <link href="~/Content/dataTableCss.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/fonts/flaticon.css" rel="stylesheet" />
    <link href="~/Content/font/flaticon.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css.map" />
    <link href="~/Content/styles.css" rel="stylesheet" />
    <script src="~/Scripts/sweetalert2.all.js"></script>
    <link rel="icon" href="~/Content/img/icon.png" type="image/png" />

    <!--Custom Font-->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">
    <!--[if lt IE 9]>
    <script src="js/html5shiv.js"></script>
    <script src="js/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <div class="loader"></div>
    <nav class="navbar navbar-custom navbar-fixed-top" role="navigation">
        <div class="">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#sidebar-collapse" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand"><img src="~/Content/img/Logo-VF-sualiado.png" id="imgS" class="img-responsive3" /></a>
                <ul class="nav navbar-top-links navbar-right">
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            <em class="flaticon-user-1"></em>
                        </a>
                        <ul class="dropdown-menu dropdown-messages">
                            <li>
                                <a href="~/Configuracion/Configuracion">Configuración</a>
                            </li>
                            <li class="divider"></li>
                            <li>
                                <a href="/Cerrar/Cerrar">Cerrar sesión</a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
        <!-- /.container-fluid -->
    </nav>

    <div id="sidebar-collapse" class="col-sm-3 col-lg-2 sidebar">
        <div class="profile-sidebar">
            <div class="profile-userpic">
                <a href="/Configuracion/Configuracion">
                    <img src="@ViewBag.perfile" class="img-responsive" alt="">
                </a>
            </div>
            <div class="profile-usertitle">
                <div class="profile-usertitle-name" id="username">@ViewBag.nombre</div>
                <div class="profile-usertitle-status"><span class="indicator label-success"></span>Online</div>
            </div>
            <div class="clear"></div>
        </div>
        <div class="divider"></div>
        <ul class="nav menu">
            @switch (ViewBag.rol)
            {
                case 1:
                    <li><a href="/Inicio/Inicio" id="inicioBtn"><em class="flaticon-home">&nbsp;</em>Inicio</a></li>
                    <li><a href="/Producto/Inventario" id="inventarioBtn"><em class="flaticon-box"></em> Productos</a></li>
                    <li><a href="/Ventas/Ventas" id="ventasBtn"><em class="flaticon-shopping-bag">&nbsp;</em>Ventas</a></li>
                    <li><a href="/Proveedor/Proveedores" id="proveedoresBtn"><em class="flaticon-contact-book">&nbsp;</em>Proveedores</a></li>
                    <li><a href="/Entradas/Entrada" id="comprasBtn"><em class="flaticon-shopping-basket">&nbsp;</em>Compras</a></li>
                    <li class="parent ">
                        <a data-toggle="collapse" href="#sub-item-1" id="reportesBtnnn">
                            <em class="flaticon-report-1">&nbsp;</em> Reportes <span data-toggle="collapse" href="#sub-item-1" class="icon pull-right"><em class="fa fa-plus"></em></span>
                        </a>
                        <ul class="children collapse" id="sub-item-1">
                            <li>
                                <a class="" onclick="Redireccionar('reportes')" value="Reportes" id="reportes">
                                    <span class="fa fa-arrow-right">&nbsp;</span> Empresa
                                </a>
                            </li>
                            <li>
                                <a class="" value="Reporte Daños" onclick="Redireccionar('daños')" id="daños">
                                    <span class="">&nbsp;</span> Daños
                                </a>
                            </li>
                        </ul>
                    </li>
                    <li><a href="/Empleado/Empleado" id="empleadosBtn"><em class="flaticon-employees">&nbsp;</em>Empleados</a></li>
                    <li><a href="/Agenda/Agenda" id="agendaBtn"><em class="flaticon-to-do-list">&nbsp;</em>Agendar Tareas</a></li>
                    break;
                case 2:
                    <li><a href="/Inicio/Inicio" id="inicioBtn"><em class="flaticon-home">&nbsp;</em>Inicio</a></li>
                    <li><a href="/Producto/Inventario" id="inventarioBtn"><em class="flaticon-box"></em> Productos</a></li>
                    <li><a href="/Ventas/Ventas" id="ventasBtn"><em class="flaticon-shopping-bag">&nbsp;</em>Ventas</a></li>
                    <li><a href="/Proveedor/Proveedores" id="proveedoresBtn"><em class="flaticon-contact-book">&nbsp;</em>Proveedores</a></li>
                    break;

                case 3:
                    <li><a href="/Inicio/Inicio" id="inicioBtn"><em class="flaticon-home">&nbsp;</em>Inicio</a></li>
                    <li>
                        <a class="" value="Reporte Daños" onclick="Redireccionar('daños')" id="daños">
                            <span class="">&nbsp;</span> Daños
                        </a>
                    </li>
                    break;
            }
        </ul>
    </div><!--/.sidebar-->

    <div class="col-sm-9 col-sm-offset-3 col-lg-10 col-lg-offset-2 main">
        @RenderBody()
    </div>	<!--/.main-->
    <a href="http://www.freepik.com">Designed by stories / Freepik</a>
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <!--<script src="js/chart.min.js"></script>
    <script src="js/chart-data.js"></script>
    <script src="js/easypiechart.js"></script>
    <script src="js/easypiechart-data.js"></script>-->
    <script src="~/Scripts/bootstrap-datepicker.js"></script>
    <script src="~/Scripts/custom.js"></script>
    <script src="~/Scripts/Inventario.js"></script>
    <script src="~/Scripts/tablas.js"></script>
    <script src="~/Scripts/Chart.js"></script>
    <script src="~/Scripts/Proveedores.js"></script>
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.21/js/jquery.dataTables.js"></script><font></font>
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.21/js/dataTables.bootstrap4.min.js"></script><font></font>
    <script>
        $(window).ready(function () {
            $(".loader").fadeOut("slow");
        });
    </script>
    <script src="~/Scripts/Rutinas.js"></script>

</body>
</html>